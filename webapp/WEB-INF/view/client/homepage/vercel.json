{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "webapp/WEB-INF/view/client/homepage/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/show.html" },
    { "src": "/product", "dest": "/product.show.html" },
    { "src": "/product/detail", "dest": "/product.detail.html" },
    { "src": "/cart/history", "dest": "/cart.history.html" }
  ]
}
